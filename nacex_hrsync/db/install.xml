<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/nacex_hrsync/db" VERSION="2024011500" COMMENT="XMLDB file for local_nacex_hrsync">
  <TABLES>
    <TABLE NAME="local_nacex_hrsync_log" COMMENT="Synchronization log">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="action" TYPE="char" LENGTH="20" NOTNULL="true" SEQUENCE="false" COMMENT="create, update, suspend, delete"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="employeeid" TYPE="char" LENGTH="50" NOTNULL="true" SEQUENCE="false" COMMENT="External HR employee ID"/>
        <FIELD NAME="details" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="status" TYPE="char" LENGTH="10" NOTNULL="true" DEFAULT="success" SEQUENCE="false" COMMENT="success or error"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="action_idx" UNIQUE="false" FIELDS="action"/>
        <INDEX NAME="employeeid_idx" UNIQUE="false" FIELDS="employeeid"/>
        <INDEX NAME="timecreated_idx" UNIQUE="false" FIELDS="timecreated"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
